{
  "$schema": "../../../node_modules/wrangler/config-schema.json",
  "name": "citychat-identity",
  "main": "identity.ts",
  "compatibility_date": "2025-05-01",
  "queues": {
    "producers": [
      { "queue": "golden", "binding": "GOLDEN_PRODUCER" }
