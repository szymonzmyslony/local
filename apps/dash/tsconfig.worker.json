{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "lib": [
      "ES2022"
    ],
    "types": [
      "node",
      "./worker-configuration.d.ts"
    ],
    "baseUrl": ".",
    "paths": {
      "@gallery-agents/shared": [
        "../../packages/shared/src/index.ts"
      ],
      "@gallery-agents/shared/*": [
        "../../packages/shared/src/*"
      ],
      "@gallery-agents/shared/ui": [
        "../../packages/shared/src/ui/index.ts"
      ],
      "@gallery-agents/shared/ui/*": [
        "../../packages/shared/src/ui/*"
      ],
      "@shared": [
        "../../packages/shared/src/index.ts"
      ],
      "@shared/*": [
        "../../packages/shared/src/*"
      ],
      "@shared/ui": [
        "../../packages/shared/src/ui/index.ts"
      ],
      "@shared/ui/*": [
        "../../packages/shared/src/ui/*"
      ]
    }
  },
  "include": [
    "worker/**/*",
    "workflows/**/*",
    "worker-configuration.d.ts",
    "workflows/utils/**/*.ts",
    "../../packages/shared/src/**/*.ts"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "../../packages/shared/src/ui/**/*"
  ]
}
