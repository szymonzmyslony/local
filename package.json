{
  "name": "gallery-agents",
  "version": "0.1.0",
  "description": "Monorepo for gallery agent workers and shared libraries",
  "type": "module",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspace @gallery-agents/shared && npm run build --workspace @gallery-agents/dash && npm run build --workspace @gallery-agents/app",
    "check": "npm run lint --workspace @gallery-agents/dash && npm run lint --workspace @gallery-agents/app && npm run typecheck",
    "format": "prettier --write .",
    "lint": "biome lint .",
    "start": "npm run dev --workspace @gallery-agents/dash",
    "dev": "npm run dev --workspace @gallery-agents/dash",
    "test": "npm run test --workspace @gallery-agents/app",
    "typecheck": "tsc -b packages/shared/tsconfig.json apps/dash/tsconfig.json apps/app/tsconfig.json"
  },
  "keywords": [
    "cloudflare",
    "ai",
    "agents"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@cloudflare/vite-plugin": "^1.13.12",
    "@cloudflare/vitest-pool-workers": "^0.9.12",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "3.2.4",
    "wrangler": "^4.45.0"
  }
}
